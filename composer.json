{
  "name": "time-chimp/timechimp-sdk",
  "description": "This is a package that allows you to call the API of TimeChimp",
  "license": "MIT",
  "authors": [
    {
      "name": "Petro Vankevych",
      "email": "petro@swipex.ua"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "^7.0.1",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Guapa\\TimeChimp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Guapa\\TimeChimp\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3.3"
  }
}
